{
  "name": "Cath Text",
  "icons": {
    "16": "icons8-notes-16.png",
    "48": "icons8-notes-48.png",
    "100": "icons8-notes-100.png"
  },
  "description": "A notes in new tab page for Google Chrome.",
  "version": "1.1",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "scripts": [
      "src/javascript/editor.js",
      "src/javascript/link.js",
      "src/javascript/tools-events.js",
      "src/javascript/main.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://*.quilljs.com; object-src 'self'",
  "manifest_version": 2,
  "author": "Catharina Mesquita",
  "homepage_url": "https://github.com/acmesquita/cath-text"
}